{% extends 'base.html.twig' %}

{% block title %}Community{% endblock %}

{% block body %}
    <h1>Community</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ community.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ community.name }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ community.city }}</td>
            </tr>
            <tr>
                <th>PostalCode</th>
                <td>{{ community.postalCode }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_community_index') }}">back to list</a>

    <a href="{{ path('app_community_edit', {'id': community.id}) }}">edit</a>

    {{ include('community/_delete_form.html.twig') }}
{% endblock %}
